---
// src/layouts/CaseStudyLayout.astro
import BaseLayout from './BaseLayout.astro'

export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseLayout title={title}>
  <main>
    <section id="case">
      <div class="container">
        <article class="case-study-content">
          <slot />
        </article>
      </div>
    </section>
  </main>
</BaseLayout>